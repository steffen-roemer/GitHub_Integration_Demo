{
  "app": {
    "base_language": "en",
    "push": {
      "source": [
        {
          "path": "./ReactJSON/en/en.json",
          "file_format": "react_nested_json",
          "tag": "app:language.json",
          "key_prefix": "react_",
          "language": "en"
        }
      ]
    },
    "pull": {
      "target": [
        {
          "path": "./ReactJSON/<language>/translated.json",
          "file_format": "react_nested_json",
          "tag": "app:language.json",
          "key_prefix": "react_",
          "exclude_languages": [
            "en"
          ]
        }
      ]
    }
  }
}